thname.split(\"/\").at(-1);\n\n    async function fetchBlogsData(){\n        setLoading(true);\n        try{\n  \n          const result = await fetch(`${baseUrl}/getblog?sidebartitle=${sidebartitle.replace(\" \", \"-\")}`);\n          const data = await result.json();\n\n          setPost(data.data);\n  \n        }\n        catch(err){\n          console.log(err);\n        }\n        setLoading(false);\n      }\n  \n      useEffect(() => {\n        fetchBlogsData();\n      },[])\n\n  return (\n    <div className='flex w-full'>\n        \n        <div className='flex flex-col'>\n        {\n          loading ? (<Spinner type={\"Loading, \"}/>) :\n          (\n            <div>\n            <p className='p-2 text-[14px]'><strong className='font-semibold '><NavLink to=\"/\"> Home </NavLink>/ <NavLink to=\"/food\"> Food  </NavLink></strong>  / {post.sidebartitle}</p>\n            <div className='flex flex-col p-5'>\n                <p className='font-semibold text-2xl underline'>{post.blogtitle}</p>\n                <div className=\"text-justify align-baseline leading-8 text-base font-normal text-[17px] py-3\">\n                {                \n                    <span dangerouslySetInnerHTML={{__html: post.description}} />\n                }\n                </div>\n            </div>\n            </div>\n          )}\n        </div>\n    </div>\n  )\n}\n\nexport default BlogPage","/Users/virendrasingh/mommygk_v1/mommygk_v1/src/components/Header.js",["125"],[],"import { useState } from \"react\";\nimport {Link, NavLink} from \"react-router-dom\"\n\nfunction Header() {\n\n  const [open, setOpen] = useState(true);\n\n  async function clickHandlerBars()\n  {\n     setOpen(false);\n  }\n\n  async function clickHandlerCross()\n  {\n    setOpen(true);\n  }\n\n  return (\n    <div>\n       <div className=\"flex top-0 justify-between items-center  mx-auto w-full  h-20 px-5\n       bg-gradient-to-r from-pink-500 via-red-500 to-yellow-500\">\n\n           <Link to=\"/\">\n   